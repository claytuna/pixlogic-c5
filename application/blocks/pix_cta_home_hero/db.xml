<?xml version="1.0" encoding="UTF-8"?>
<schema
  xmlns="http://www.concrete5.org/doctrine-xml/0.5"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">

  <table name="btPixCtaHomeHero">
    <field name="bID" type="integer">
      <unsigned/>
      <key/>
    </field>
    <field name="fID" type="integer">
      <unsigned/>
      <default value="0"/>
    </field>
    <field name="title" type="string" size="255"/>
    <field name="description" type="text"/>
    <field name="externalLink" type="string" size="255"/>
    <field name="internalLinkCID" type="integer">
      <unsigned/>
      <default value="0"/>
    </field>
    <field name="externalLink2" type="string" size="255"/>
    <field name="internalLinkCID2" type="integer">
      <unsigned/>
      <default value="0"/>
    </field>
    <field name="altText" type="text"/>
    <field name="altText2" type="text"/>
    <index name="fID">
      <col>fID</col>
    </index>
  </table>

</schema>
